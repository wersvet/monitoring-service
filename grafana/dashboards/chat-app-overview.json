{
  "uid": "chat-app-overview",
  "title": "Chat App Overview",
  "tags": ["chat", "overview"],
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "10s",
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "templating": {
    "list": [
      {
        "name": "service",
        "type": "query",
        "label": "Service",
        "datasource": "Prometheus",
        "refresh": 1,
        "hide": 0,
        "query": "label_values(http_requests_total, service)",
        "multi": true,
        "includeAll": true,
        "allValue": ".*",
        "current": {
          "text": "All",
          "value": "$__all"
        }
      }
    ]
  },
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": "-- Grafana --",
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "panels": [
    {
      "id": 1,
      "type": "timeseries",
      "title": "HTTP RPS",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "sum(rate(http_requests_total{service=~\"$service\"}[1m])) by (service)",
          "legendFormat": "{{service}}"
        }
      ],
      "gridPos": {"x": 0, "y": 0, "w": 8, "h": 8}
    },
    {
      "id": 2,
      "type": "timeseries",
      "title": "HTTP Error Rate (5xx)",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "sum(rate(http_requests_total{status=~\"5..\",service=~\"$service\"}[1m])) by (service)",
          "legendFormat": "{{service}}"
        }
      ],
      "gridPos": {"x": 8, "y": 0, "w": 8, "h": 8}
    },
    {
      "id": 3,
      "type": "timeseries",
      "title": "HTTP Latency P95",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket{service=~\"$service\"}[5m])) by (le, service))",
          "legendFormat": "{{service}}"
        }
      ],
      "gridPos": {"x": 16, "y": 0, "w": 8, "h": 8}
    },
    {
      "id": 4,
      "type": "table",
      "title": "Top Paths by RPS",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "topk(10, sum(rate(http_requests_total{service=~\"$service\"}[1m])) by (path))",
          "legendFormat": "{{path}}",
          "format": "table"
        }
      ],
      "transformations": [
        {
          "id": "labelsToFields",
          "options": {}
        }
      ],
      "gridPos": {"x": 0, "y": 8, "w": 24, "h": 8}
    },
    {
      "id": 5,
      "type": "timeseries",
      "title": "WS Active Connections",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "ws_connections_active{service=\"chat-service\"}",
          "legendFormat": "active"
        }
      ],
      "gridPos": {"x": 0, "y": 16, "w": 8, "h": 8}
    },
    {
      "id": 6,
      "type": "timeseries",
      "title": "WS Opened per Min",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "rate(ws_connections_opened_total{service=\"chat-service\"}[1m])",
          "legendFormat": "opened"
        }
      ],
      "gridPos": {"x": 8, "y": 16, "w": 8, "h": 8}
    },
    {
      "id": 7,
      "type": "timeseries",
      "title": "WS Closed per Min",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "rate(ws_connections_closed_total{service=\"chat-service\"}[1m])",
          "legendFormat": "closed"
        }
      ],
      "gridPos": {"x": 16, "y": 16, "w": 8, "h": 8}
    },
    {
      "id": 8,
      "type": "timeseries",
      "title": "WS Messages Sent per Min",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "rate(ws_messages_sent_total{service=\"chat-service\"}[1m])",
          "legendFormat": "sent"
        }
      ],
      "gridPos": {"x": 0, "y": 24, "w": 12, "h": 8}
    },
    {
      "id": 9,
      "type": "timeseries",
      "title": "WS Messages Received per Min",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "rate(ws_messages_received_total{service=\"chat-service\"}[1m])",
          "legendFormat": "received"
        }
      ],
      "gridPos": {"x": 12, "y": 24, "w": 12, "h": 8}
    }
  ]
}
